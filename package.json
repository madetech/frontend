{
  "name": "@madetech/frontend",
  "version": "1.1.6",
  "description": "A collection of components and resources for creating Made Tech branded sites and applications.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madetech/frontend.git"
  },
  "bugs": {
    "url": "https://github.com/madetech/frontend/issues"
  },
  "homepage": "https://github.com/madetech/frontend#readme",
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "release": "npm-run-all release:commit release:tag release:push",
    "release:commit": "git add package.json* && git commit -m 'New release v$npm_package_version'",
    "release:tag": "git tag v$npm_package_version",
    "release:push": "git push origin master v$npm_package_version",
    "lib:build": "babel src --out-dir lib",
    "sass:build": "npm-run-all sass:build:compile sass:build:min",
    "sass:build:compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/madetech-frontend.scss dist/css/madetech-frontend.css",
    "sass:build:min": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output dist/css/madetech-frontend.min.css dist/css/madetech-frontend.css",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "author": "Made Tech <hello@madetech.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@madetech/marketing-assets": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "bootstrap": "^4.1.3",
    "clean-css-cli": "^4.2.1",
    "jquery": "^3.3.1",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-scripts": "^2.1.1",
    "react-styleguidist": "^8.0.4"
  }
}
